#!/usr/bin/env bash
# shellcheck disable=SC1091
source ember_bash_setup &> /dev/null
#set -x

trap 'die "A fatal error was reported on ${BASH_SOURCE[0]} line ${LINENO}."' ERR

directory="."
[[ -n "$1" ]] && directory="$1"

cd "$directory"
[[ -e ".sreg/.sreg.conf"]]

print "${EmberLibrary:?}/Futuramerlin Projects/Data/Stream Registry/"
