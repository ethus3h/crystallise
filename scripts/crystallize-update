#!/usr/bin/env bash
# shellcheck disable=SC1091
source ember_bash_setup &> /dev/null
#set -x

((EUID)) && exec sudo -- "$0" "$@"
trap 'die "A fatal error was reported on ${BASH_SOURCE[0]} line ${LINENO}."' ERR

mkdir 2017oct03; cd 2017oct03; git clone https://github.com/ethus3h/ember-shared.git; cd ember-shared; make; sudo make install; cd ..; git clone https://github.com/ethus3h/crystallize.git; cd crystallize; make; sudo make noconf
