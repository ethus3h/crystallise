#!/bin/bash
[ "$(whoami)" != "root" ] && exec sudo -- "$0" "$@"
crystalWorkdir=$(crystallize-getconf 'WorkDirectory')
(
    cd "$crystalWorkdir"
    sponge "$streamId.xz"
    unxz --stdout "$streamId.xz" | tar -xf -
    biggify --here "$streamId.crystal" &> /dev/null
    tail -c +37 "$streamId.tmp"
)
